{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es6"],
    "types": ["cypress"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["cypress/**/*.ts"]
} 